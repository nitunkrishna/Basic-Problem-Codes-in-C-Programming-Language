#include<stdio.h>
#include<string.h>
int main()
{
    char s[1000], t[1000];
    scanf("%s %s", s, t);
    int m=strlen(s), n=strlen(t);
    for(int index=0; index<m-n+1; index++)
    {
        int flag=1;
        for(int i=0; i<n; i++)
        {
            if(t[i]!=s[index+i]) flag=0;
        }
        if(flag==1)
        {
            printf("Yes\n");
            return 0;
        }
    }
    printf("NO\n");
return 0;
}
